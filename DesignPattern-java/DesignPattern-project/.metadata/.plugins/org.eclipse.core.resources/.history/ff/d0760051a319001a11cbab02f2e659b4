
public class ThroughputScheduler implements ElevatorScheduler{
	public static ThroughputScheduler throughputScheduler;
	
	public static ThroughputScheduler getInstance() {
		if(throughputScheduler == null)
			throughputScheduler = new ThroughputScheduler();
		
		return throughputScheduler;
	}
	
	public int selectedElevator(ElevatorManager manager,int destination,Direction direction) {
		return 0;
	}
}
