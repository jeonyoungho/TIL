
public class Client {

	public static void main(String[] args) {
		CarComponent basicCar = new BasicCar();
		
		System.out.println("Car Price : " + basicCar.getPrice());
		System.out.println("Car Info : " + basicCar.getCarinfo());
		
		CarComponent basicCarWithAirBag = new AirBagDecorator(basicCar, 10000);
		
		System.out.println("Car Price : " + basicCar.getPrice());
		System.out.println("Car Info : " + basicCar.getCarinfo());		
	}

}
